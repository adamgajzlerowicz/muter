{
  "manifest_version": 2,
  "name": "YouMute(r)",
  "short_name": "YouMute(r)",
  "version": "1.3",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "description": "Chrome plugin that automatically mutes youtube commercials.",
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/*"
      ],
      "js": [
        "muter.js"
      ]
    }
  ]
}